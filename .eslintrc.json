{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "jest/globals": true
    },
    "extends": ["eslint:recommended", "prettier", "plugin:react/recommended"],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["react", "jest"],
    "rules": {
        "react/react-in-jsx-scope": "off",
        "react/prop-types": 0,
        "no-unused-vars": "warn",
        "no-var": "error"
    },
    "settings": {
        "react": {
            "version": "detect"
        }
    }
}
